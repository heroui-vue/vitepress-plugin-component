{
  "name": "vitepress-plugin-vue-component-view",
  "version": "1.0.0",
  "description": "VitePress plugin that renders Vue components with tabs showing preview and source code",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./client": "./src/client.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vitepress",
    "plugin",
    "vue",
    "component",
    "demo",
    "tabs"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/vitepress-plugin-component-tabs.git"
  },
  "peerDependencies": {
    "markdown-it": "^12.0.0 || ^13.0.0",
    "vitepress": "^1.0.0",
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@highlightjs/vue-plugin": "^2.1.0",
    "highlight.js": "^11.11.1",
    "markdown-it-container": "^3.0.0"
  },
  "devDependencies": {
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.15.11",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitepress": "^1.0.0-beta.1",
    "vue": "^3.3.4"
  },
  "tsup": {
    "entry": [
      "src/index.js"
    ],
    "format": [
      "esm"
    ],
    "dts": false,
    "clean": true,
    "outDir": "dist"
  }
}
