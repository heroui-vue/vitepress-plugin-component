{
  "name": "vitepress-plugin-vue-component-view",
  "version": "0.2.2",
  "description": "VitePress plugin that renders Vue components with tabs showing preview and source code",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./client": "./dist/client.js",
    "./styles": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "types": "index.d.ts",
  "scripts": {
    "dev": "vite -w build",
    "build": "vite build",
    "dev:tsdown": "tsdown -w",
    "build:tsdown": "tsdown",
    "pub": "tsdown && bumpp --no-push --no-tag && npm publish --registry=https://registry.npmjs.org/"
  },
  "keywords": [
    "vitepress",
    "plugin",
    "vue",
    "component",
    "demo",
    "tabs"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "hotdogc1017@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nextui-vue/vitepress-plugin-vue-component-view"
  },
  "dependencies": {
    "highlight.js": "^11.11.1",
    "markdown-it-container": "^3.0.0",
    "unplugin-lightningcss": "^0.3.3",
    "vite": "^6.3.5"
  },
  "devDependencies": {
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-container": "^2.0.10",
    "@types/node": "^18.15.11",
    "bumpp": "^10.1.0",
    "tsdown": "latest",
    "typescript": "^5.8.3",
    "unplugin-vue": "^6.2.0",
    "vitepress": "^1.0.0-beta.1",
    "vue": "^3.3.4"
  }
}
